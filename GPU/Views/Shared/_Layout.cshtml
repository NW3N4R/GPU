<!DOCTYPE html>
<html lang="en" dir="rtl">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>
    @* <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" asp-append-version="true" /> *@
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/DetailsCss.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/NavCss.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/StudentInsertStyle.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/StudentTableStyle.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/InvoicesStyle.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/HomeStyle.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/StaticalSearchBar.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/StaticalTableSpecialStyle.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/NewInvoiceStyle.css" asp-append-version="true" />
    @* <link rel="stylesheet" href="~/GPU/styles.css" asp-append-version="true" /> *@
    <link rel="icon" type="image/x-icon" href="~/favicon.ico">
    <link rel="icon" type="image/png" sizes="32x32" href="~/logo.png">
    <link rel="icon" type="image/png" sizes="16x16" href="~/logo.png">
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>

</head>
<body>
    <header>
        <nav>
            <div class="NavRight">
                <ul class="nav-nav flex-grow-1">
                    <li class="nav-li ">
                        <a class="nav-link  "
                           asp-area=""
                           asp-controller="Home"
                           asp-action="Index">سەرەتا</a>
                    </li>
                    <li class="nav-li ">
                        <a class="nav-link  "
                        asp-area="" 
                        asp-controller="Students" 
                        asp-action="Index">خوێندکاران</a>
                    </li>
                    <li class="nav-li ">
                        <a class="nav-link "
                        asp-area=""
                        asp-controller="Archive"
                        asp-action="Index">ئەرشیف</a>
                    </li>
                 
                    <li class="nav-li ">
                        <a class="nav-link "
                           asp-area=""
                           asp-controller="StaticalView"
                           asp-action="Index">ئامار</a>
                    </li>

                       <li class="nav-li ">
                        <a class="nav-link "
                        asp-area="" 
                        asp-controller="Students"
                        asp-action="Create">تۆمارکردن</a>
                    </li>

                    <li class="nav-li ">
                        <a class="nav-link "
                           asp-area=""
                           asp-controller="Home"
                           asp-action="Index">بڕوانامەکان</a>
                    </li>
                </ul>

            </div>
            <div class="NavCenter">
                <a class="ActionSide" asp-area="" asp-controller="Home" asp-action="Index">
                    <img src="~/logo.png" class="NavLogo" />
                </a>
            </div>
            <div class="NavLeft">
                <a class="ActionSide" asp-area="" asp-controller="Home" asp-action="Index">
                    <div class="NavTxt">
                        <p>پەیمانگای تەکنیکی کەلار</p>
                        <p class="BottomSpan">هۆبـەی تـۆمـارگـا</p>
                    </div>
                </a>

            </div>
        </nav>
    </header>
    <div class="container">

        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="finner">
            <span>
                هۆبەی تۆمارگای پەیمانگای تەکنیکی کەلار  &copy;
            </span>
            <span style="margin-right:40px;">
                Developed By Hiwa A. Abbas
            </span>

        </div>

    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js" asp-append-version="true"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js" asp-append-version="true"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @* <script src="~/js/StudentInsertJS.js" asp-append-version="true"> </script> *@
    <script src="~/js/HomeJS.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>


